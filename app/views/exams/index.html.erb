<p id="notice"><%= notice %></p>


<%= render 'layouts/adminsection' %>
<h1>Listing Exams</h1>  <!-- || <%#= link_to 'Userdetail' , user_path(@user) %> 
 -->

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exams.each do |exam| %>
      <tr>
        <td><%= exam.name %></td>
        <td><%= link_to 'Show', exam %></td> |
        <td><%= link_to 'Edit', edit_exam_path(exam) %></td> |
        <td><%= link_to 'Destroy', exam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exam', new_exam_path %>
<%= will_paginate @exams %>